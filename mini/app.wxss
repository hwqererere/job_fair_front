.access{position:fixed;z-index:99999;left:0rpx;top:0rpx;width:100%;height:100%;background:none}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}


.hide {
  display: none;
}

.form-group {
  border-bottom: #ddd solid 1px;
  position: relative;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  padding: 20rpx 0;
}

.form-label {
  float:left;margin-left:20rpx;
  line-height: 50rpx;
  font-size: 28rpx;
}

.form-input {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  border:none; 
}

.form-save .btn{
   background: #e83743;
   width:70%;
   height: 80rpx;
   line-height: 80rpx;
   border-radius:  80rpx;
   color: #fff;
   font-size: 30rpx;
}
.bottom-pad{
    height: 110rpx;
    background: #fff;
    width: 100%;
}

.laybg{position:fixed;width:100%;height:100%;left:0rpx;top:0rpx;z-index:999;background-color:#000;opacity:0.7}




.header-search {
  height: 64rpx;
  float:left;
  width: 100%;
  
}

.header-search .search-box {
  float:left;width:calc(100% - 40rpx);
  height: 64rpx;padding:0rpx 20rpx;position:fixed;left:0rpx;top:0rpx;background-color:#357ac0;z-index: 999;

}

.header-search .search-input {
  border-radius: 24rpx;float:left;margin-top:4rpx;width:480rpx;
  background: #fff;
  text-align: center;
  color: #c37d70;
  font-size: 24rpx;
  height: 48rpx;
  line-height: 48rpx;
}
.header-search .search{float:left;height:64rpx;margin-left:-50rpx;width:60rpx;line-height:56rpx; }
.header-search .icon{float:left;height:64rpx;margin-left:10rpx;width:60rpx;line-height:64rpx;color:#fff}
.header-search .scan {
  height:64rpx;float:left;line-height:64rpx;
  width: 60rpx;margin-right:20rpx;
  color:#fff;
}


