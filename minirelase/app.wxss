page{background-color:#fff;font-size:24rpx;}
view,input,icon{font-family: 'iconfont';}
.access{position:fixed;z-index:99999;left:0rpx;top:0rpx;width:100%;height:100%;background:none}
.container{height: 100%;}
.hide{display: none;}
.bottom-pad{height: 110rpx; background: #fff; width: 100%;}
.laybg{position:fixed;width:100%;height:100%;left:0rpx;top:0rpx;z-index:999;background-color:#000;opacity:0.7}

.footbar{position:fixed;height:100rpx;bottom:0rpx;left:0rpx;width:100%;background-color:#fff;border-top:1rpx solid #ddd}
.footbar .v{float:left;width:33%;height:100rpx;}
.footbar .v view{float:left;width:100%;height:40rpx;line-height:40rpx;font-size:28rpx;text-align:center;color:#525252;}
.footbar .v.current view{color:#002570}
.footbar .v view:first-child{margin-top:20rpx;font-size:40rpx}
.grayline{float:left;width:100%;height:20rpx;background-color:#f8f8f8;margin:0rpx 0rpx;}
.grayline2{float:left;width:100%;height:10rpx;background-color:#f8f8f8;margin:0rpx 0rpx;}
