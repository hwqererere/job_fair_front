.compname{flaot:left;width:100%;height:80rpx;line-height:80rpx;font-size:40rpx;color:#525252;text-align:center;margin-top:20rpx;}
.baseline{float:left;width:calc(100% - 60rpx);margin-top:10rpx;padding-left:30rpx;padding-right:30rpx}
.baseline label{float:left;font-size:30rpx}
.baseline label:nth-child(1){width:200rpx;margin-left:25rpx;text-align:left}
.baseline label:nth-child(2){width:50rpx;margin-left:-60rpx;}
.baseline label:nth-child(3){width:500rpx;margin-left:-40rpx;}
.baseline label.k2{letter-spacing:70rpx}
.baseline label.k3{letter-spacing:21rpx}
.baseline label.k4{letter-spacing:5rpx}
.baseline label.pt{padding-top:5rpx}
.baseline label input{font-size:36rpx;color:#999}
.baseline picker{float:left;width:100%;height:60rpx;}
.baseline picker view{float:left;width:100%;height:60rpx;color:#ccc}
.baseline textarea{float:left;width:480rpx;line-height:60rpx;}
.baseline .for1,.for2{float:left;}
.baseline .for2{background-color:#e1f3fe;padding:5rpx 10rpx;margin:10rpx 10rpx;border-radius:3rpx;color:#79bea2;font-size:30rpx}
.baseline textarea{height:auto;min-height:600rpx;}
.icon-icon_task{color:#006633;float:left;font-size:30rpx;margin-top:20rpx}
.savebtn{float:left;width:100%;height:80rpx;line-height: 80rpx;text-align:center;font-size:36rpx;border-top:1rpx solid #ddd;color:#0080ff;white-space:10rpx;}

.fulilay{position:fixed;left:0rpx;top:0rpx;height:100%;width:100%;z-index:999;background-color:#fff}
.fulisel{float:left;border:1px solid #e5eaff;padding:10rpx 14rpx;color:#0080ff;border-radius:10rpx;width:100rpx;margin:14rpx 20rpx; }
.fulisel.sel{background-color:#0080ff;color:#fff}
.addinput{float:left;width:100%;border-top:1rpx solid #ddd;padding-top:30rpx;margin-top:10rpx;}
.addinput input{float:left;width:300rpx;border:1rpx solid #003366;margin-left:10rpx;border-radius:10rpx;height:40rpx; }
.addinput .btn{height:60rpx;line-height:60rpx;font-size:30rpx;text-align:center;color:#fff;padding:0rpx 20rpx;background-color:#0080ff;width:100rpx;border-radius:10rpx;margin-left:20rpx;float:left;}
.fulilayscrollview{float:left;width:calc(100% - 50rpx);height:700rpx;padding-top:40rpx;padding-left:25rpx;padding-right:25rpx;}
.addline{float:left;width:100%;margin-top:20rpx;}
.addline .btn{width:140rpx;float:left;margin-left:305rpx;color:#0080ff;border-radius:10rpx;height:60rpx;line-height:60rpx;text-align:center;letter-spacing:10rpx;}