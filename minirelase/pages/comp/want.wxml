<view class="li" wx:for="{{deli}}" wx:if="{{item.status==4}}">
    <navigator class="img" url="resume?openid={{item.openid}}">
      <image src="{{reslink+item.url_id}}"></image>
    </navigator>
    <view class="msg">
      <label>{{item.username}}|{{item.sex==0?"女":"男"}}</label>
      <label>{{item.jobName}}</label>
    </view>
  </view>